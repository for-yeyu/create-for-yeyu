{
  "name": "create-for-yeyu",
  "version": "1.0.0",
  "description": "A CLI tool to scaffold projects from templates",
  "type": "module",
  "bin": {
    "create-for-yeyu": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cli",
    "scaffold",
    "template"
  ],
  "author": "yeyu",
  "license": "MIT",
  "dependencies": {
    "@types/chalk": "^2.2.4",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "degit": "^2.8.4",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/degit": "^2.8.6",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
